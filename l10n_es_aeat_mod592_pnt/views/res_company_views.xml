<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="aeat592_res_company_inh_form" model="ir.ui.view">
        <field name="name">aeat592.res.company.inh.form</field>
        <field name="model">res.company</field>
        <field name="type">form</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='website']" position="after">
                <field name="pnt_plastic_tax" widget="monetary"/>
                <field name="pnt_plastic_journal_id"/>
                <field name="pnt_plastic_commercial_account_id"/>
                <field name="pnt_plastic_manufacture_account_id"/>
            </xpath>
        </field>
    </record>

</odoo>
